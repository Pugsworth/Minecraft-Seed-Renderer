{
    "files.associations": {
        "*.lua": "lua",
        "util.h": "c",
        "generator.h": "c",
        "types.h": "c",
        "stat.h": "c"
    },
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/lib/**"
    ],
    "cmake.configureOnOpen": true,
    "cmake.preferredGenerators": [
        "MinGW Makefiles"
    ],
    "cmake.configureOnEdit": false,
}